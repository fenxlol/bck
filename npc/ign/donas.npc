				case 2:
					if(#ESCOCT == 1) {
						mes "Lo siento, pero ya haz obtenido tus premios, vuelve luego.";
						close2; cutin "",255; end;
					}
					if(#ESCALA < 1) {
						mes "[^5533ffDonaciones^000000]";
						mes "Lo siento pero no tienes puntos para la escala de Premios.";
						close2; cutin "",255; end;
					}
					if(#ESCALA >= 50) {
						setarray .@Ea$[0],"Balloon al azar","50 Mora Coins","30 Event Ticket","Witch's Pumpkin Hat","50 IGN Coin";
