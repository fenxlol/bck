							set .@m$,.@m$+":";
						}
						mes "[^5533ffDonaciones^000000]";
						mes "Por favor selecciona el objeto que desees obtener:";
						next;
						set .@ie,select(.@m$)-1;
						mes "[^5533ffDonaciones^000000]";
						mes "Haz elegido:";
						mes "^ff0000"+.@Ea$[.@ie]+"^000000.";
						mes " ";
						mes "¿Estás segur"+(Sex?"o":"a")+"?";
						next;
						if(select("Continuar","^777777Salir^000000") == 2) {
							mes "[^5533ffDonaciones^000000]";
							mes "Bien, aquí guardaré tus premios, vuelve pronto.";
							set .@m$,"";
							set .@ie,0;
							set .@i,0;
							close2;
							cutin "",255;
							end;
						}
						setarray .@EsA[0],.@Ea[.@ie],.@Ea_c[.@ie];
						if(#ESCALA >= 100) {
							set .@ib,getescala(100);
							setarray .@EsB[0],.@Eb[.@ib],.@Eb_c[.@ib];
						}
						if(#ESCALA >= 200) {
							set .@ic,getescala(200);
							setarray .@EsC[0],.@Ec[.@ic],.@Ec_c[.@ic];
						}
						if(#ESCALA >= 500) {
							set .@id,getescala(500);
							setarray .@EsD[0],.@Ed[.@id],.@Ed_c[.@id];
						}
						mes .npc$;
						mes "Bien, aquí están tus objetos.";
						next;
						set #ESCALA,0;
						specialeffect 154;
						getitem .@EsA[0],.@EsA[1];
						next;
						specialeffect 154;
						if(.@EsB[0] > 0)
							getitem .@EsB[0],.@EsB[1];
						next;
						specialeffect 154;
						if(.@EsC[0] > 0)
							getitem .@EsC[0],.@EsC[1];
						next;
						specialeffect 154;
						if(.@EsD[0] > 0)
