						mes "Gracias por tu apoyo.";
						close2;
						deletearray .@EsA,getarraysize(.@EsA);
						deletearray .@EsB,getarraysize(.@EsB);
						deletearray .@EsC,getarraysize(.@EsC);
						deletearray .@EsD,getarraysize(.@EsD);
						set @ib,0;
						set @id,0;
						set @ic,0;
						set #ESCOCT,1;
						emotion e_thx;
						cutin "",255;
						end;
					}
