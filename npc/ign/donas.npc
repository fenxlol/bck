function	getescala	{
	mes "[^5533ffDonaciones^000000]";
	set @b$,"";
	deletearray @Ea,getarraysize(@Ea);
	deletearray @Ea,getarraysize(@Ea_c);
	set @size,0;
	switch (getarg(0)) {
		case 100:
			set @b$,"segunda";
			setarray .@Ea[0],35035,35036,35005,12922,7711;
			setarray .@Ea_c[0],2,2,1,5,50;
			set @size,getarraysize(@Ea)-1;
			break;
		case 200:
			set @b$,"tercera";
			setarray .@Ea[0],13912,13913,13914;
			setarray .@Ea_c[0],30,30,30;
			set @size,getarraysize(@Ea)-1;
			break;
		case 500: 
			setarray .@Ea[0],35059,20707,2589,35034;
			setarray .@Ea_c[0],1,1,1,1;
			set @size,getarraysize(@Ea)-1;
			set @b$,"última";
			break;
	}
	mes "Bien, comencemos con la "+@b$+" ronda.";
	next;
	set @m$,"";
	for(set @i,0; @i <= @size; set @i,@i+1) {
		set @m$,@m$+"^5533ff~^000000"+@Ea_c[@i]+"^008800x^000000 "+getitemname(@Ea[@i]);
		set @m$,@m$+":";
	}
	mes "[^5533ffDonaciones^000000]";
	mes "Por favor selecciona el objeto que desees obtener:";
	next;
	set @ie,select(@m$)-1;
	mes "[^5533ffDonaciones^000000]";
	mes "Haz elegido:";
	mes "^ff0000"+@Ea_c[@ie]+"x "+getitemname(@Ea[@ie])+"^000000.";
	mes " ";
	mes "¿Estás segur"+(Sex?"o":"a")+"?";
	next;
	if(select("Continuar","^777777Salir^000000") == 2) {
		mes "[^5533ffDonaciones^000000]";
		mes "Bien, aquí guardaré tus premios, vuelve pronto.";
		set @m$,"";
		set @ie,0;
		set @i,0;
		close2; cutin "",255; end;
	}
	return @ie;
}
OnInit:
	set .waiting,120000; // Tiempo de cada mensaje.
