						setarray .@Eb[0],35035,35036,35005,12922,7711;
						setarray .@Eb_c[0],2,2,1,5,50;
						setarray .@Ec[0],13912,13913,13914;
						setarray .@Ec_c[0],30,30,30;
						setarray .@Ed[0],35059,20707,2589,35034;
						setarray .@Ed_c[0],1,1,1,1;
						mes "[^5533ffDonaciones^000000]";
						mes "Bien, comencemos con la primera ronda.";
						next;
						set .@m$,"";
						for(set .@i,0; .@i <= getarraysize(.@Ea$)-1; set .@i,.@i+1) {
