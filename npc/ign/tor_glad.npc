//===========[ Menu de GM ] ====================
	if(getgroupid() >= 60) {
		mes .npc$;
		mes "¿Qué desea?";
		next;
		switch(select("Continuar:Definir Rango")) {
			case 1:
				break;
			case 2:
				input @rank;
				set Rango,@rank;
				dispbottom "Rango "+@rank+" fijado.";
				break;
		}
	}
//==============================================
	if(!Rango || Rango < 0)
		set Rango,0;
	mes .npc$;
	mes "Bienvenid"+(Sex?"o ^0000ff":"a ^ff0000")+" "+strcharinfo(0)+"^5533ff "+.rangos$[Rango];
	mes "^000000Me encargo de la organización de los torneos para que valientes luchen contra numerosos monstruos.";
	next;
	mes .npc$;
	mes "Dime, ¿estás interesad"+(Sex?"o":"a")+"?";
	next;
	switch (select("Participar en el Torneo:Tienda de Puntos de Gladiador:^ff0000Información^000000:^777777Salir^000000")) {
		case 1:
			set .@map_menu$,"";
			for(set .@m,1; .@m <= getarraysize(.@ME$)-1; set .@m,.@m+1) {
				set .@map_menu$,.@map_menu$+.@ME_n$[.@m]+"~ [^"+(getmapusers(.@ME$[.@m]) ? "ff0000Ocupado":"008800Disponible")+"^000000]";
				set .@map_menu$,.@map_menu$+":";
			}
			mes .npc$;
			mes "Por favor selecciona el mapa en el que quieras participar:";
			next;
			set .@me,select(.@map_menu$);
			if(getmapusers(.@ME$[.@me]) > 0) {
				mes .npc$;
				mes "Lo siento, pero actualmente la sala se encuentra ocupada, intenta dentro de unos minutos hasta que esté libre.";
				close2;
				emotion e_sry;
				end;
			}
			mes .npc$;
			mes "Excelente.";
			mes "Dime, ¿dónde participarás?";
			next;
			switch(select("Torneos ^ff6060[ "+getarraysize(.torneo_name$)+" ]^000000:^777777Salir^000000")) {
				case 1:
					set .@menu$,"";
					for(set .@i,0; .@i <= Rango; set .@i,.@i+1) {
						set .@menu$,.@menu$+.torneo_name$[.@i];
						set .@menu$,.@menu$+":";
					}
					set .@torneo,select(.@menu$)-1;
					mes .npc$;
					mes "Estás a punto de entrar a:";
					mes "^ff0000"+.torneo_name$[.@torneo]+"^000000";
					mes " ";
					mes "En el mapa:";
					mes "^cc0000"+.@ME_n$[.@me]+"("+.@ME$[.@me]+")^000000";
					next;
					if(select("Empezar","^777777Salir^000000") == 2) {
						mes .npc$;
						mes "Bien, regresa cuando en verdad quieras participar.";
						close;
					}
					mes .npc$;
					mes "¡Bien, hallá vamos!";
					mes "Cuando entres a la sala presta atención a los anuncios, o los monstruos te tomarán por sorpresa.";
					close2;
					killmonsterall .@ME$[.@me];
					warp .@ME$[.@me],49,45;
					sleep2 1000;
					mapannounce .@ME$[.@me],"[Organizador]: Prepárense para el espectáculo",bc_map;
					sleep2 6000;
					mapannounce .@ME$[.@me],"[Organizador]: El "+.torneo_name$[.@torneo]+" está a punto de empezar",bc_map;
					sleep2 6000;
					mapannounce .@ME$[.@me],"[Organizador]: Veamos qué clase de espectáculo nos ofrece.",bc_map;
					sleep2 8000;
