								monster mapa$,0,0,"Lava Golem",1366,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 7:
								set mbt,12;
								set rnd,1;
								callsub S_Check,rnd,"Whisper",mapa$;
								monster mapa$,0,0,"Whisper",1179,12,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 8:
								set mbt,33;
								set rnd,1;
								callsub S_Check,rnd,"Desérticos",mapa$;
								monster mapa$,0,0,"Baby Leopard",1415,5,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,0,0,"Choco",1214,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,0,0,"Desert Wolf",1106,5,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,0,0,"Gullinbursti",1311,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,0,0,"Side Winder",1037,5,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,0,0,"Yoyo",1057,15,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,0,0,"Vagabond Wolf",1092,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
