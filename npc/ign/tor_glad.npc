								monster strcharinfo(3),50,56,"Hermano Mayor",1050,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 1:
								set mbt,3;
								set rnd,1;
								callsub S_Check,rnd,"Trio Mortal",strcharinfo(3);
								monster strcharinfo(3),41,50,"Trio Mortal",1122,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),58,50,"Trio Mortal",1122,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),50,57,"Trio Mortal",1122,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 2:
								set mbt,81;
								set rnd,1;
								callsub S_Check,rnd,"Equipo Arcoiris",strcharinfo(3);
								monster strcharinfo(3),49,45,"Poring",1002,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),49,45,"Poporing",1031,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),49,45,"Marin",1242,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),49,45,"Drops",1113,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),49,45,"Mastering",1090,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 3:
								set mbt,10;
								set rnd,1;
								callsub S_Check,rnd,"Virus",strcharinfo(3);
								monster strcharinfo(3),0,0,"Virus",1627,10,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 4:
								set mbt,20;
								set rnd,1;
								callsub S_Check,rnd,"Esqueleto Orco",strcharinfo(3);
								monster strcharinfo(3),0,0,"Esqueleto Orco",1152,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 5:
								set mbt,13;
								set rnd,1;
								callsub S_Check,rnd,"Lobos",strcharinfo(3);
								monster strcharinfo(3),0,0,"Vagabond Wolf",1092,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),0,0,"Wolf",1013,12,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 6:
								set mbt,10;
								set rnd,1;
								callsub S_Check,rnd,"Flamiferos",strcharinfo(3);
