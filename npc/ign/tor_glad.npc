								callsub S_Check,rnd,"Equipo Arcoiris",mapa$;
								monster mapa$,49,45,"Poring",1002,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,49,45,"Poporing",1031,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,49,45,"Marin",1242,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,49,45,"Drops",1113,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,49,45,"Mastering",1090,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 3:
								set mbt,10;
								set rnd,1;
								callsub S_Check,rnd,"Virus",mapa$;
								monster mapa$,0,0,"Virus",1627,10,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 4:
								set mbt,20;
