								monster mapa$,50,56,"Hermano Mayor",1050,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 1:
								set mbt,3;
								set rnd,1;
								callsub S_Check,rnd,"Trio Mortal",mapa$;
								monster mapa$,41,50,"Trio Mortal",1122,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,58,50,"Trio Mortal",1122,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,50,57,"Trio Mortal",1122,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 2:
