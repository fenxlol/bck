								callsub S_Check,rnd,"Esqueleto Orco",mapa$;
								monster mapa$,0,0,"Esqueleto Orco",1152,20,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 5:
								set mbt,13;
