	if(getarraysize(.@ME$) != getarraysize(.@ME_n$) || (getarraysize(.torneo_name$)-1) != getarraysize(.rangos$)) {
		debugmes "ARRAYS ARE NOT EQUAL: .@ME$~.@ME_n$~.torneo_name$~.rangos$";
		mes "^ff0000Ha ocurrido un error grave, menciónale un administrador que revise la consola^000000";
		close;
	}
