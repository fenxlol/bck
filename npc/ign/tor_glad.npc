//=====[ Funciones Globales ]======
S_Check: //,rnd,"Mob Name",MapID;
	if(mapa$ == "t_glad01" || mapa$ == "t_glad02" || mapa$ == "t_glad03" || mapa$ == "t_glad04" || mapa$ == "t_glad05") {
		if(getmapusers(getarg(2)) > 1) {
			mapannounce getarg(2),"Lo sentimos pero sólo puede haber un concursante por sala.",bc_map;
			sleep2 3000;
			warp "prontera",162,170;
			end;
		}
		mapannounce getarg(2),"Ronda "+getarg(0)+" ~ "+strcharinfo(0)+" Vs. "+getarg(1)+".",bc_map;
		sleep2 5000;
		return;
	}
	dispbottom "Torneo de Gladiadores cancelado por salir del mapa.";
	end;
S_Points:
	if(mapa$ == "t_glad01" || mapa$ == "t_glad02" || mapa$ == "t_glad03" || mapa$ == "t_glad04" || mapa$ == "t_glad05") {
		soundeffect "complete.wav",0;
		mapannounce getarg(1),"[Organizador]: Has completado satisfactoriamente el "+.torneo_name$[tgID],bc_map;
		set GPoints,GPoints + getarg(0);
		dispbottom "Has obtenido "+getarg(0)+" Puntos de Gladiador.";
		sleep2 5000;
			if(getarg(3)) {
				getitem getarg(3),getarg(4);
				dispbottom "Has obtenido "+getarg(4)+" x "+getitemname(getarg(3))+".";
			}
		if(Rango < getarg(2)) {
			set Rango,getarg(2);
			dispbottom "¡Tu rango como Gladiador ha aumentado!";
			sleep2 1000;
			soundeffect "lady_tanee_attack.wav",0;
			dispbottom "Nuevo Rango: "+.rangos$[Rango];
			sleep2 1000;
			mapannounce "arena_room","[Organizador]: "+strcharinfo(0)+" ha conseguido el rango de "+.rangos$[Rango],bc_map;
		}
	}
	dispbottom "Monstruo inválido.";
	return;
S_Ruleta:
		setarray .@felic$[0],"Perfecto","Excelente","Eres "+(Sex?"el":"la")+" mejor","Asombroso","Alucinante","Increíble","Oh Poring";
		mapannounce mapa$,"[Organizador]: ¡"+.@felic$[rand(0,getarraysize(.@felic$)-1)]+"! pasas a la siguiente ronda.",bc_map;
		sleep2 1000;
		mapannounce mapa$,"La siguiente ronda comenzará dentro de poco, prepárate.",bc_map;
		soundeffect "megalith_die.wav",0;
		sleep2 700;
		dispbottom "¡¡Recuperación de HP y SP 20%!!";
		percentheal 20,20;
		soundeffect "gemini_attack.wav",0;
		soundeffect "gemini_attack.wav",0;
		dispbottom "------------------------------------------";
		dispbottom "      ~~ Ruleta de Estados ~~";
		dispbottom "------------------------------------------";
