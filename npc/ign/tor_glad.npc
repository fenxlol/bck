			mes " ";
			mes "Propón nuevos objetos a comerciar a los Game Master.";
			next;
			switch(select("200 P.Glad => ^ff880020 PvP Coin^000000")) {
				// PuntosGlad,CantidadItem,Item
				case 1: callsub S_Exchange,250,40,677;
			}
