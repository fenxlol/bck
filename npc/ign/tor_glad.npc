								monster strcharinfo(3),0,0,"Pasana",1154,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),0,0,"Lava Golem",1366,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 7:
								set mbt,12;
								set rnd,1;
								callsub S_Check,rnd,"Whisper",strcharinfo(3);
								monster strcharinfo(3),0,0,"Whisper",1179,12,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 8:
								set mbt,33;
								set rnd,1;
								callsub S_Check,rnd,"Desérticos",strcharinfo(3);
								monster strcharinfo(3),0,0,"Baby Leopard",1415,5,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),0,0,"Choco",1214,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),0,0,"Desert Wolf",1106,5,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),0,0,"Gullinbursti",1311,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
