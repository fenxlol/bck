								callsub S_Check,rnd,"Lobos",mapa$;
								monster mapa$,0,0,"Vagabond Wolf",1092,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster mapa$,0,0,"Wolf",1013,12,"TG1::OnRound"+rnd+"Torneo"+tgID;
								end;
							case 6:
								set mbt,10;
								set rnd,1;
								callsub S_Check,rnd,"Flamiferos",mapa$;
								monster mapa$,0,0,"Blazer",1367,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
