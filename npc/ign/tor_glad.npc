			next;
			switch(select("^5533ff150 Pun. Glad.^000000 ^ff0000=>^000000 ^00880050 Event Coin^000000:^5533ff250 Pun. Glad.^000000 ^ff0000=>^000000 ^00880040 PvP Coin^000000:^5533ff300 Pun. Glad.^000000 ^ff0000=>^000000 ^0088001 Emperium Coin^000000:^5533ff100 Pun. Glad.^000000 ^ff0000=>^000000 ^0088005 MvP Coin^000000:^5533ff100 Pun. Glad.^000000 ^ff0000=>^000000 ^00880010 Event Ticket^000000")) {
				case 1: callsub S_Exchange,150,50,675;
				case 2: callsub S_Exchange,250,40,677;
				case 3: callsub S_Exchange,300,1,674;
				case 4: callsub S_Exchange,100,5,673;
				case 5: callsub S_Exchange,100,10,7711;
			}
		case 3:
			while(1) {
				mes .npc$;
				mes "¿Qué te gustaría saber o conocer sobre el ^cc6600Torneo de Gladiadores^000000?";
				next;
					switch(select("Información General:Puntos de Gladiador:Ruleta de Estados:Modos:^777777Salir^000000")) {
						case 1:
							mes .npc$;
							mes "En este lugar organizamos los torneos de gladiadores donde probamos tu fuerza y valentía en el campo de batalla.";
							next;
							mes .npc$;
							mes "En cada etapa deberás de enfrentar a uno o varios monstruos que su misión principal es hacerte perder, deberás de hacer uso de tus habilidades para eliminarles a todos.";
							next;
							mes .npc$;
							mes "Dependiendo del nivel de dificultad el número de etapas incrementa y se hará más dificil pasar cada torneo.";
							break;
						case 2:
							mes .npc$;
							mes "Éstos puntos los obtendrás al completar cada torneo, pero sólo te serán útiles aquí.";
							next;
							mes .npc$;
							mes "Dependiendo de la cantidad de puntos que tengas podrás intercambiarlo por distintos objetos.";
							next;
							mes .npc$;
							mes "Al completar cada torneo tu ^5533ffRango de Gladiador^000000 incrementará, ésto te permitirá acceder a torneos mejores y más difíciles donde podrás ganar aún más puntos de los que ganabas en torneos anteriores.";
							break;
						case 3:
							mes .npc$;
							mes "Cada vez que termines una batalla en la ^0000CCArena de Gladiadores^000000, se activará la ^0000CCRuleta de Estados^000000.";
							next;
							mes .npc$;
							mes "Esto añadirá una posibilidad de recibir un estado beneficioso, como por ejemplo: Bendición, Aumentar Agilidad, Resistir... y muchas más.";
							next;
							mes .npc$;
							mes "Además de eso al activarse la ^0000CCRuleta de Estados^000000 también te curaras un 20% de tu HP y SP máximo independientemente de lo que salga en la ruleta.";
							break;
						case 4:
							mes .npc$;
							mes "Además de los torneos también podrás participar en algunos eventos especiales llamados ^0000CCModos^000000, las reglas de cada uno són diferentes entre si.";
							next;
							mes .npc$;
							mes "A diferencia de los torneos aqui no aumentarás tu ^0000CCRango de Gladiador^000000 y solo podrás obtener más ^0000CCModos^000000 aumentando tu ^0000CCRango de Gladiador^000000.";
							break;
						case 5:
							mes .npc$;
							mes "Vuelve cuando desees más ayuda o volver a repasar estos contenidos.";
							close2;
							emotion e_no1;
							end;
					}
			}
		case 4:
			mes .npc$;
			mes "De acuerdo, vuelve cuando desees.";
			close2;
			emotion e_ok;
			end;
	}
end;

