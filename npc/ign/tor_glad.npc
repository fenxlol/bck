							break;
						case 5:
							mes .npc$;
							mes "Vuelve cuando desees más ayuda o volver a repasar estos contenidos.";
							close2;
							emotion e_no1;
							end;
					}
			}
		case 4:
			mes .npc$;
			mes "De acuerdo, vuelve cuando desees.";
			close2;
			emotion e_ok;
			end;
	}
end;

