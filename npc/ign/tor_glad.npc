								monster strcharinfo(3),0,0,"Explosion",1383,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),0,0,"Grand Peco",1369,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),0,0,"Heater",1318,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
								monster strcharinfo(3),0,0,"Kaho",1072,1,"TG1::OnRound"+rnd+"Torneo"+tgID;
